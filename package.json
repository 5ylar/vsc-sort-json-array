{
    "name": "sort-json-array",
    "displayName": "sort-json-array",
    "description": "Sorts a json array by property",
    "version": "0.0.1",
    "publisher": "fvclaus",
    "engines": {
        "vscode": "^1.26.0"
    },
    "repository": {
        "url": "https://github.com/fvclaus/vsc-sort-json-array"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sortJsonArray"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sortJsonArray",
                "title": "Sort JSON array"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
